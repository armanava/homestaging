              },
              "& when (@iconsBg)": {
                "background-color": "@imgIconsBgColor"
              }
            },
            ".gallery-item": {
              "& when (@flexDirection)": {
                "padding-left": "0",
                "padding-right": "0"
              }
            },
            ".gallery-row, .gallery-item": {
